                </TabItem>

                <TabItem Height="42">
                    <TabItem.Resources>
                        <Style TargetType="TabItem">
                            <Setter Property="Template">
                                <Setter.Value>
                                    <ControlTemplate TargetType="TabItem">
                                        <Border Name="Border" BorderThickness="0,0,0,0" BorderBrush="White" CornerRadius="5,0,0,5">
                                            <TextBlock x:Name="TB" Text="Climate" Style="{StaticResource TabStyle}"/>
                                        </Border>
                                        <ControlTemplate.Triggers>
                                            <Trigger Property="IsSelected" Value="True">
                                                <Setter TargetName="Border" Property="Background" Value="White"/>
                                                <Setter TargetName="TB" Property="TextBlock.Foreground" Value="Red"/>
                                                <Setter TargetName="Border" Property="Border.Effect">
                                                    <Setter.Value>
                                                        <DropShadowEffect Color="Black" Direction="-90" ShadowDepth="1" BlurRadius="5" Opacity="0.8" />
                                                    </Setter.Value>
                                                </Setter>
                                            </Trigger>
                                            <MultiTrigger>
                                                <MultiTrigger.Conditions>
                                                    <Condition Property="IsMouseOver" Value="True"/>
                                                    <Condition Property="IsSelected" Value="False"/>
                                                </MultiTrigger.Conditions>
                                                <Setter TargetName="Border" Property="Background" Value="Red"/>
                                            </MultiTrigger>
                                            <MultiTrigger>
                                                <MultiTrigger.Conditions>
                                                    <Condition Property="IsMouseOver" Value="True"/>
                                                    <Condition Property="IsSelected" Value="True"/>
                                                </MultiTrigger.Conditions>
                                                <Setter TargetName="Border" Property="Background" Value="White"/>
                                            </MultiTrigger>
                                        </ControlTemplate.Triggers>
                                    </ControlTemplate>
                                </Setter.Value>
                            </Setter>
                        </Style>
                    </TabItem.Resources>

                    <StackPanel Orientation="Vertical">
                        <TextBlock Text="Climatic Conditions "
                               FontSize="32"
                               FontWeight="Light"
                               FontStyle="Italic"
                               FontFamily="Times New Roman"
                               Foreground="#FF9211E8"
                               VerticalAlignment="Top"
                               HorizontalAlignment="Left"
                               Margin="20,20,0,0">
                        </TextBlock>

                        <StackPanel Orientation="Horizontal" Margin="10,25,0,0">
                            <Label Content="Type of Climate " Width="220" Style="{StaticResource LabelStyle}"/>
                            <TextBox Text="{Binding newQPanelData.ClimateType}" Width="250" Style="{StaticResource InputStyle}" />
                        </StackPanel>

                        <StackPanel Orientation="Horizontal" Margin="10,15,0,0">
                            <Label Content="Type of Atmosphere " Width="220" Style="{StaticResource LabelStyle}"/>
                            <TextBox Text="{Binding newQPanelData.AtmosphereType}" Width="250" Style="{StaticResource InputStyle}" />
                        </StackPanel>

                        <StackPanel Orientation="Horizontal" Margin="10,15,0,0">
                            <Label Content="Pollution Risk " Width="220" Style="{StaticResource LabelStyle}"/>
                            <TextBox Text="{Binding newQPanelData.PollutionRisks}" Width="250" Style="{StaticResource InputStyle}" />
                        </StackPanel>

                        <StackPanel Orientation="Horizontal" Margin="10,15,0,0">
                            <Label Content="Ambient Temperature " Width="220" Style="{StaticResource LabelStyle}"/>
                            <TextBox Text="{Binding newQPanelData.AmbientTemperature }" Width="100" Style="{StaticResource InputStyle}" HorizontalContentAlignment="Center" PreviewKeyDown="DoubleOnly_PreviewKeyDown" />
                            <Label Content="C˚" Width="50" Style="{StaticResource LabelStyle}" HorizontalContentAlignment="Left"/>
                        </StackPanel>

                        <StackPanel Orientation="Horizontal" Margin="10,15,0,0">
                            <Label Content="Relative Humidity " Width="220" Style="{StaticResource LabelStyle}"/>
                            <TextBox Text="{Binding newQPanelData.RelativeHumidity}" Width="100" Style="{StaticResource InputStyle}" HorizontalContentAlignment="Center" PreviewKeyDown="DoubleOnly_PreviewKeyDown"  />
                            <Label Content="%" Width="50" Style="{StaticResource LabelStyle}" HorizontalContentAlignment="Left"/>
                        </StackPanel>

                        <StackPanel Orientation="Horizontal" Margin="10,15,0,0">
                            <Label Content="Altitude (Above Sea Level)" Width="220" Style="{StaticResource LabelStyle}"/>
                            <TextBox Text="{Binding newQPanelData.SeaLevel}" Width="100" Style="{StaticResource InputStyle}" HorizontalContentAlignment="Center" PreviewKeyDown="DoubleOnly_PreviewKeyDown"  />
                            <Label Content="M" Width="50" Style="{StaticResource LabelStyle}" HorizontalContentAlignment="Left"/>
                        </StackPanel>


                    </StackPanel>
                </TabItem>

                <TabItem Height="42">
                    <TabItem.Resources>
                        <Style TargetType="TabItem">
                            <Setter Property="Template">
                                <Setter.Value>
                                    <ControlTemplate TargetType="TabItem">
                                        <Border Name="Border" BorderThickness="0,0,0,0" BorderBrush="White" CornerRadius="5,0,0,5">
                                            <TextBlock x:Name="TB" Text="Particular Note" Style="{StaticResource TabStyle}"/>
                                        </Border>
                                        <ControlTemplate.Triggers>
                                            <Trigger Property="IsSelected" Value="True">
                                                <Setter TargetName="Border" Property="Background" Value="White"/>
                                                <Setter TargetName="TB" Property="TextBlock.Foreground" Value="Red"/>
                                                <Setter TargetName="Border" Property="Border.Effect">
                                                    <Setter.Value>
                                                        <DropShadowEffect Color="Black" Direction="-90" ShadowDepth="1" BlurRadius="5" Opacity="0.8" />
                                                    </Setter.Value>
                                                </Setter>
                                            </Trigger>
                                            <MultiTrigger>
                                                <MultiTrigger.Conditions>
                                                    <Condition Property="IsMouseOver" Value="True"/>
                                                    <Condition Property="IsSelected" Value="False"/>
                                                </MultiTrigger.Conditions>
                                                <Setter TargetName="Border" Property="Background" Value="Red"/>
                                            </MultiTrigger>
                                            <MultiTrigger>
                                                <MultiTrigger.Conditions>
                                                    <Condition Property="IsMouseOver" Value="True"/>
                                                    <Condition Property="IsSelected" Value="True"/>
                                                </MultiTrigger.Conditions>
                                                <Setter TargetName="Border" Property="Background" Value="White"/>
                                            </MultiTrigger>
                                        </ControlTemplate.Triggers>
                                    </ControlTemplate>
                                </Setter.Value>
                            </Setter>
                        </Style>
                    </TabItem.Resources>

                    <StackPanel Orientation="Vertical">

                        <TextBlock Text="Particular Note "
                               FontSize="32"
                               FontWeight="Light"
                               FontStyle="Italic"
                               FontFamily="Times New Roman"
                               Foreground="#FF9211E8"
                               VerticalAlignment="Top"
                               HorizontalAlignment="Left"
                               Margin="20,20,0,0">
                        </TextBlock>

                        <StackPanel Orientation="Horizontal" Margin="10,10,0,0">
                            <Label Content="Apparatus Defind " Width="150" Style="{StaticResource LabelStyle}"/>
                            <TextBox Text="{Binding newQPanelData.ApparatusDefind}" Width="100" Style="{StaticResource InputStyle}" HorizontalContentAlignment="Center"/>
                        </StackPanel>

                        <StackPanel Orientation="Horizontal" Margin="10,10,0,0">
                            <Label Content="Weight " Width="150" Style="{StaticResource LabelStyle}"/>
                            <TextBox Text="{Binding newQPanelData.Weight}" Width="100" Style="{StaticResource InputStyle}" HorizontalContentAlignment="Center" />
                            <Label Content="kg" Width="50" Style="{StaticResource LabelStyle}" HorizontalContentAlignment="Left"/>
                        </StackPanel>

                        <StackPanel Background="DarkGray" HorizontalAlignment="Left" Orientation="Horizontal" Margin="30,15,0,0">
                            <Label Content="Drawing: " Width="250" Style="{StaticResource LabelStyle}" Foreground="White" HorizontalContentAlignment="Left"/>
                        </StackPanel>
                        <StackPanel HorizontalAlignment="Left" Orientation="Horizontal" Margin="30,0,0,0">
                            <CheckBox IsChecked="{Binding newQPanelData.ForInformation}" Width="50" Style="{StaticResource CheckBoxStyle}"/>
                            <Label Content="For Information " Width="200" Style="{StaticResource LabelStyle}" HorizontalContentAlignment="Left"/>
                        </StackPanel>
                        <StackPanel Background="#FFF1F1F1" HorizontalAlignment="Left" Orientation="Horizontal" Margin="30,0,0,0">
                            <CheckBox IsChecked="{Binding newQPanelData.ForProduction}" Width="50" Style="{StaticResource CheckBoxStyle}"/>
                            <Label Content="For Approval " Width="200" Style="{StaticResource LabelStyle}" HorizontalContentAlignment="Left"/>
                        </StackPanel>
                        <StackPanel HorizontalAlignment="Left" Orientation="Horizontal" Margin="30,0,0,0">
                            <CheckBox IsChecked="{Binding newQPanelData.ForApproval}" Width="50" Style="{StaticResource CheckBoxStyle}"/>
                            <Label Content="For Production " Width="200" Style="{StaticResource LabelStyle}" HorizontalContentAlignment="Left"/>
                        </StackPanel>
                        <StackPanel Background="#FFF1F1F1" HorizontalAlignment="Left" Orientation="Horizontal" Margin="30,0,0,0">
                            <CheckBox IsChecked="{Binding newQPanelData.AsManufactured}" Width="50" Style="{StaticResource CheckBoxStyle}"/>
                            <Label Content="As Manufactured" Width="200" Style="{StaticResource LabelStyle}" HorizontalContentAlignment="Left"/>
                        </StackPanel>

                        <StackPanel Orientation="Horizontal" Margin="10,10,0,0">
                            <Label Content="Remarks " Width="120" Style="{StaticResource LabelStyle}" HorizontalContentAlignment="Center"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Margin="35,0,0,0">
                            <TextBox Text="{Binding newQPanelData.Remarks}" Width="300" Style="{StaticResource InputStyle}" MaxHeight="110" MinHeight="30" TextWrapping="Wrap"/>
                        </StackPanel>

                    </StackPanel>

                </TabItem>

            </TabControl>

            <StackPanel Orientation="Horizontal" Grid.Row="1" HorizontalAlignment="Right">
                <Button Click="Save_Click" Style="{StaticResource SaveButton}"/>
                <Button Click="Cancel_Click" Style="{StaticResource CancelButton}"/>
            </StackPanel>
        </Grid>
    </Border>
</Window>
